{#ADMIN#}

{% extends "EDRAppliBundle::layoutadmin.html.twig" %}

{% block body %}
    <h3 class="admin_h3">Tag</h3>
<div class="row">
    <div class="col s12 col m10 offset-m2"
    {{ tag.nom }}
    <p class="admin_nav">
        <a href="{{ path('tag_edit', { 'id': tag.id }) }}" class="admin_edit">Modifier</a>
        <a href="{{ path('tag_index') }}" alt="Retour à la liste" title="Retour à la liste" class="admin_retour">&crarr;</a>
        {{ form_start(delete_form) }}
        <input type="submit" value="Delete" class="admin_delete">
        {{ form_end(delete_form) }}

    </p>
    </div>
</div>

{% endblock %}
