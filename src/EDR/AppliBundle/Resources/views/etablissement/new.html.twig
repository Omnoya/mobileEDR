{#ADMIN#}

{#{% extends 'base.html.twig' %} #}
{% extends "EDRAppliBundle::layoutadmin.html.twig" %}


{% block body %}
    <h3 class="admin_h3">Ajouter un nouvel établissement</h3>

<div class="admin_form">
    {{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" value="Ajouter" class="admin_valider"/>
    <a href="{{ path('etablissement_index') }}" alt="Retour à la liste des établissements" title="Retour à la liste des établissements">&crarr;</a>
    {{ form_end(form) }}
</div>

{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script>
        $(document).ready(function() {
            $('select').material_select();
        });
    </script>

{% endblock %}