{# src/EDR/AppliBundle/Resources/views/Appli/index.html.twig #}

{% extends "EDRAppliBundle::layout.html.twig" %}

{% block title  %}
    Recherche Restaurants - {{ parent() }}
{% endblock %}

{% block menu %}

    <div class="row">
        <div class="offset-s10 col s2">
            <i class="material-icons home_profil">account_circle</i>
        </div>

        <hr />

        <div class="offset -s1 col s10 offset -s1">
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d83998.75786844667!2d2.277345044075076!3d48.858950577385116!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47e66e1f06e2b70f%3A0x40b82c3688c9460!2sParis!5e0!3m2!1sfr!2sfr!4v1469530479595" width="352" height="264" frameborder="0" style="border:0" allowfullscreen></iframe>
        </div>

    </div>

    <input id="pao" type="text" id="input" placeholder="type whatever" value="#countdown" title="type and press enter" />


    <ul class="nav nav-pills nav-stacked">
        {% for categorie in categories %}
            <div class="col s12">
                <div class="row">
                    <div>
                        <li class="class_{{ categorie.nom|lower }}"><a href="{{ path('etab_category_show', {'category' : categorie.nom }) }}"><p class="home_center"><i class="material-icons logo">{{ categorie.icongoogle }}</i></p></a></li>
                    </div>
                </div>
            </div>
        {% endfor %}
    </ul>


{% endblock  %}


